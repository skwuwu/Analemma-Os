{
    "workflow_name": "test_llm_operator_integration_workflow",
    "description": "LI-D: LLM + operator_official 통합 테스트",
    "version": "1.0.0",
    "nodes": [
        {
            "id": "start",
            "type": "operator_official",
            "config": {
                "strategy": "timestamp",
                "params": {
                    "format": "iso"
                },
                "output_key": "start_time"
            }
        },
        {
            "id": "generate_json",
            "type": "llm_chat",
            "config": {
                "provider": "gemini",
                "model": "gemini-2.0-flash",
                "system_prompt": "You are a task manager. Generate a JSON array of tasks with priorities. Respond ONLY with valid JSON.",
                "prompt_content": "Generate 5 software development tasks. Each task should have: id (string), title (string), priority ('High', 'Medium', or 'Low'), and estimated_hours (number). Return as a JSON object with a 'tasks' array.",
                "max_tokens": 1024,
                "temperature": 0.5,
                "response_schema": {
                    "type": "object",
                    "properties": {
                        "tasks": {
                            "type": "array",
                            "items": {
                                "type": "object",
                                "properties": {
                                    "id": {
                                        "type": "string"
                                    },
                                    "title": {
                                        "type": "string"
                                    },
                                    "priority": {
                                        "type": "string",
                                        "enum": [
                                            "High",
                                            "Medium",
                                            "Low"
                                        ]
                                    },
                                    "estimated_hours": {
                                        "type": "number"
                                    }
                                },
                                "required": [
                                    "id",
                                    "title",
                                    "priority"
                                ]
                            }
                        }
                    }
                },
                "output_key": "llm_json_output"
            }
        },
        {
            "id": "parse_json",
            "type": "operator_official",
            "config": {
                "strategy": "json_parse",
                "input_key": "llm_json_output",
                "params": {
                    "strict": false,
                    "default": {
                        "tasks": []
                    }
                },
                "output_key": "parsed_tasks"
            }
        },
        {
            "id": "filter_high_priority",
            "type": "operator_official",
            "config": {
                "strategy": "list_filter",
                "input": "{{parsed_tasks.tasks}}",
                "params": {
                    "condition": "$.priority == 'High'"
                },
                "output_key": "high_priority_tasks"
            }
        },
        {
            "id": "extract_titles",
            "type": "operator_official",
            "config": {
                "strategy": "list_map",
                "input_key": "high_priority_tasks",
                "params": {
                    "field": "title"
                },
                "output_key": "high_priority_titles"
            }
        },
        {
            "id": "count_results",
            "type": "operator_official",
            "config": {
                "strategy": "list_reduce",
                "input_key": "high_priority_tasks",
                "params": {
                    "operation": "count"
                },
                "output_key": "high_priority_count"
            }
        },
        {
            "id": "sum_hours",
            "type": "operator_official",
            "config": {
                "strategy": "list_reduce",
                "input": "{{parsed_tasks.tasks}}",
                "params": {
                    "operation": "sum",
                    "field": "estimated_hours"
                },
                "output_key": "total_hours"
            }
        },
        {
            "id": "finalize",
            "type": "operator_official",
            "config": {
                "strategy": "merge_objects",
                "params": {
                    "objects": [
                        {
                            "status": "COMPLETE"
                        },
                        {
                            "test_type": "llm_operator_integration"
                        },
                        {
                            "strategies_used": [
                                "json_parse",
                                "list_filter",
                                "list_map",
                                "list_reduce"
                            ]
                        }
                    ]
                },
                "output_key": "final_status"
            }
        }
    ],
    "edges": [
        {
            "source": "start",
            "target": "generate_json"
        },
        {
            "source": "generate_json",
            "target": "parse_json"
        },
        {
            "source": "parse_json",
            "target": "filter_high_priority"
        },
        {
            "source": "filter_high_priority",
            "target": "extract_titles"
        },
        {
            "source": "extract_titles",
            "target": "count_results"
        },
        {
            "source": "count_results",
            "target": "sum_hours"
        },
        {
            "source": "sum_hours",
            "target": "finalize"
        }
    ],
    "start_node": "start",
    "initial_state": {
        "generate_json_list": true
    }
}