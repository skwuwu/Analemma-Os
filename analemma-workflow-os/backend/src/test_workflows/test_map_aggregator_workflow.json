{
    "workflow_name": "Map Aggregator Test",
    "description": "Map State and Aggregator logic verification (result aggregation after parallel execution)",
    "nodes": [
        {
            "id": "parallel_node",
            "type": "parallel_group",
            "config": {
                "branches": [
                    {
                        "branch_id": "branch_A",
                        "nodes": [
                            {
                                "id": "branch_A_step1",
                                "type": "operator_official",
                                "config": {
                                    "strategy": "echo",
                                    "input": "Success A",
                                    "output_key": "branch_A_result"
                                }
                            },
                            {
                                "id": "branch_A_step1_2",
                                "type": "operator_official",
                                "config": {
                                    "strategy": "echo",
                                    "input": 12345,
                                    "output_key": "timestamp_A"
                                }
                            }
                        ]
                    },
                    {
                        "branch_id": "branch_B",
                        "nodes": [
                            {
                                "id": "branch_B_step1",
                                "type": "operator_official",
                                "config": {
                                    "strategy": "echo",
                                    "input": "Success B",
                                    "output_key": "branch_B_result"
                                }
                            },
                            {
                                "id": "branch_B_step1_2",
                                "type": "operator_official",
                                "config": {
                                    "strategy": "echo",
                                    "input": 67890,
                                    "output_key": "timestamp_B"
                                }
                            }
                        ]
                    }
                ]
            }
        },
        {
            "id": "aggregator_node",
            "type": "aggregator",
            "config": {
                "merge_policy": "FLATTEN_ALL"
            }
        },
        {
            "id": "validator_node",
            "type": "operator_official",
            "config": {
                "strategy": "echo",
                "input": "SUCCESS: All branch results aggregated",
                "output_key": "TEST_RESULT"
            }
        }
    ],
    "edges": [
        {
            "source": "parallel_node",
            "target": "aggregator_node"
        },
        {
            "source": "aggregator_node",
            "target": "validator_node"
        }
    ],
    "start_node": "parallel_node"
}