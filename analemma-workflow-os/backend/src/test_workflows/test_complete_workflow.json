{
    "workflow_name": "Complete Test Workflow",
    "description": "Normal completion simulation test - including LLM segment division",
    "nodes": [
        {
            "id": "complete_prep",
            "type": "operator",
            "sets": {
                "prep_done": true,
                "prep_result": {
                    "status": "ready_for_completion"
                }
            }
        },
        {
            "id": "complete_llm",
            "type": "aiModel",
            "config": {
                "model": "claude-3-haiku-20240307",
                "max_tokens": 100,
                "prompt": "Reply with exactly one word: COMPLETE",
                "temperature": 0.0
            }
        },
        {
            "id": "validator_node",
            "type": "operator",
            "sets": {
                "TEST_RESULT": "SUCCESS: complete test workflow finished"
            }
        }
    ],
    "edges": [
        {
            "source": "complete_prep",
            "target": "complete_llm",
            "type": "normal"
        },
        {
            "source": "complete_llm",
            "target": "validator_node",
            "type": "normal"
        }
    ],
    "start_node": "complete_prep"
}