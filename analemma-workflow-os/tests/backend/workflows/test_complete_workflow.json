{
    "workflow_name": "Complete Test Workflow",
    "description": "Normal completion simulation test - including LLM segment division",
    "nodes": [
        {
            "id": "complete_prep",
            "type": "operator_official",
            "config": {
                "strategy": "echo",
                "input": "ready_for_completion",
                "output_key": "status"
            }
        },
        {
            "id": "complete_prep_2",
            "type": "operator_official",
            "config": {
                "strategy": "echo",
                "input": true,
                "output_key": "prep_done"
            }
        },
        {
            "id": "complete_llm",
            "type": "aiModel",
            "config": {
                "model": "claude-3-sonnet",
                "max_tokens": 300,
                "prompt": "완료 상태를 확인하고 COMPLETE 결과를 생성해주세요. 준비 상태: {prep_result}",
                "temperature": 0.3
            }
        },
        {
            "id": "validator_node",
            "type": "operator_official",
            "config": {
                "strategy": "if_else",
                "params": {
                    "condition": "$.prep_done == true",
                    "then": "SUCCESS",
                    "else": "FAIL"
                },
                "output_key": "TEST_RESULT"
            }
        }
    ],
    "edges": [
        {
            "source": "complete_prep",
            "target": "complete_prep_2"
        },
        {
            "source": "complete_prep_2",
            "target": "complete_llm",
            "type": "normal"
        },
        {
            "source": "complete_llm",
            "target": "validator_node",
            "type": "normal"
        }
    ],
    "start_node": "complete_prep"
}