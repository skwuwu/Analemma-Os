{
    "workflow_name": "Multimodal Vision Test Workflow",
    "description": "Production-mode Gemini Vision API test. Operator nodes use sets shorthand only — no code execution required.",
    "nodes": [
        {
            "id": "vision_prep",
            "type": "operator",
            "config": {
                "sets": {
                    "vision_input": {
                        "image_uri": "s3://test-bucket/sample_product.jpg",
                        "analysis_type": "product_features"
                    },
                    "vision_test_enabled": true
                }
            }
        },
        {
            "id": "vision_analysis",
            "type": "aiModel",
            "config": {
                "model": "gemini-1.5-pro-vision",
                "max_tokens": 500,
                "prompt": "이미지를 분석하여 제품의 주요 특징을 한국어로 설명해주세요.\n이미지: {vision_input.image_uri}",
                "temperature": 0.3,
                "multimodal": true,
                "vision_enabled": true,
                "output_key": "vision_analysis_result"
            }
        },
        {
            "id": "vision_validator",
            "type": "operator",
            "config": {
                "sets": {
                    "vision_features_detected": true,
                    "TEST_RESULT": "✅ SUCCESS: Vision 테스트 정상 완료 (Gemini Vision 실제 호출)"
                }
            }
        }
    ],
    "edges": [
        {
            "source": "vision_prep",
            "target": "vision_analysis",
            "type": "normal"
        },
        {
            "source": "vision_analysis",
            "target": "vision_validator",
            "type": "normal"
        }
    ],
    "start_node": "vision_prep"
}
