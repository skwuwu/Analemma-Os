{
    "workflow_name": "Multimodal Vision Test Workflow",
    "description": "Production-mode Gemini Vision API test. Operator nodes use sets shorthand only — no code execution required.",
    "nodes": [
        {
            "id": "vision_prep",
            "type": "operator_official",
            "config": {
                "strategy": "echo",
                "input": true,
                "output_key": "vision_prep_done"
            }
        },
        {
            "id": "vision_analysis",
            "type": "aiModel",
            "config": {
                "model": "gemini-1.5-pro-vision",
                "max_tokens": 500,
                "prompt": "이미지를 분석하여 제품의 주요 특징을 한국어로 설명해주세요.\n이미지: {vision_input.image_uri}",
                "temperature": 0.3,
                "multimodal": true,
                "vision_enabled": true,
                "output_key": "vision_analysis_result"
            }
        },
        {
            "id": "vision_validator",
            "type": "operator_official",
            "config": {
                "strategy": "echo",
                "input": true,
                "output_key": "vision_validator_done"
            }
        }
    ],
    "edges": [
        {
            "source": "vision_prep",
            "target": "vision_analysis",
            "type": "normal"
        },
        {
            "source": "vision_analysis",
            "target": "vision_validator",
            "type": "normal"
        }
    ],
    "start_node": "vision_prep"
}