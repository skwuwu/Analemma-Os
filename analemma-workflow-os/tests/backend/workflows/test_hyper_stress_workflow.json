{
    "workflow_name": "Hyper Stress V3 Test Workflow",
    "description": "V3 stress test with nested Map, Multi-HITL, and partial sync",
    "nodes": [
        {
            "id": "stress_init",
            "type": "operator_official",
            "config": {
                "strategy": "echo",
                "input": [
                    2,
                    3,
                    2,
                    3
                ],
                "output_key": "inner_items_per_outer"
            }
        },
        {
            "id": "stress_init_2",
            "type": "operator_official",
            "config": {
                "strategy": "echo",
                "input": [
                    1,
                    3
                ],
                "output_key": "multi_hitl_points"
            }
        },
        {
            "id": "stress_init_3",
            "type": "operator_official",
            "config": {
                "strategy": "echo",
                "input": {
                    "nested_map_count": 0,
                    "hitl_count": 0
                },
                "output_key": "stress_metrics"
            }
        },
        {
            "id": "outer_map_processor",
            "type": "for_each",
            "config": {
                "input_list_key": "outer_loop_items",
                "output_key": "outer_map_results",
                "max_iterations": 5,
                "sub_workflow": {
                    "nodes": [
                        {
                            "id": "stress_outer_unit",
                            "type": "operator_official",
                            "config": {
                                "strategy": "echo",
                                "input": true,
                                "output_key": "stress_outer_unit_done"
                            }
                        }
                    ]
                }
            }
        },
        {
            "id": "hitl_checkpoint",
            "type": "operator_official",
            "config": {
                "strategy": "echo",
                "input": "AUTO_APPROVED",
                "output_key": "hitl_status"
            }
        },
        {
            "id": "stress_validator",
            "type": "operator_official",
            "config": {
                "strategy": "echo",
                "input": true,
                "output_key": "stress_validator_done"
            }
        }
    ],
    "edges": [
        {
            "source": "stress_init",
            "target": "stress_init_2"
        },
        {
            "source": "stress_init_2",
            "target": "stress_init_3"
        },
        {
            "source": "stress_init_3",
            "target": "outer_map_processor",
            "type": "normal"
        },
        {
            "source": "outer_map_processor",
            "target": "hitl_checkpoint",
            "type": "normal"
        },
        {
            "source": "hitl_checkpoint",
            "target": "stress_validator",
            "type": "normal"
        }
    ],
    "start_node": "stress_init"
}