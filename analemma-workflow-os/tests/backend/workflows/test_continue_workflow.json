{
    "workflow_name": "Continue Test Workflow",
    "description": "다음 단계 진행 시뮬레이션 테스트",
    "nodes": [
        {
            "id": "continue_step1",
            "type": "operator_official",
            "config": {
                "strategy": "echo",
                "input": 1,
                "output_key": "step"
            }
        },
        {
            "id": "continue_step1_2",
            "type": "operator_official",
            "config": {
                "strategy": "echo",
                "input": "ready_for_next",
                "output_key": "status"
            }
        },
        {
            "id": "continue_step1_3",
            "type": "operator_official",
            "config": {
                "strategy": "echo",
                "input": true,
                "output_key": "step1_done"
            }
        },
        {
            "id": "continue_step2",
            "type": "aiModel",
            "config": {
                "model": "claude-3-sonnet",
                "max_tokens": 500,
                "prompt": "2단계 처리를 완료하고 CONTINUE 상태를 확인해주세요. 상태: {step1_result}",
                "temperature": 0.7
            }
        },
        {
            "id": "validator_node",
            "type": "operator_official",
            "config": {
                "strategy": "if_else",
                "params": {
                    "condition": "$.step1_done == true",
                    "then": "SUCCESS",
                    "else": "FAIL"
                },
                "output_key": "TEST_RESULT"
            }
        }
    ],
    "edges": [
        {
            "source": "continue_step1",
            "target": "continue_step1_2"
        },
        {
            "source": "continue_step1_2",
            "target": "continue_step1_3"
        },
        {
            "source": "continue_step1_3",
            "target": "continue_step2",
            "type": "normal"
        },
        {
            "source": "continue_step2",
            "target": "validator_node",
            "type": "normal"
        }
    ],
    "start_node": "continue_step1"
}